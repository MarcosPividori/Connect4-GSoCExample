<meta http-equiv="refresh" content="5" >
$maybe _ <- maid
 <div .centre> <b>Your current auth ID:</b> #{show maid} <a href=@{AuthR LogoutR}>Logout</a>
 <div .centre> <b>Playing against:</b> #{show user2}
 <div .centre> <b>Turn:</b> #{show turn}
 <div .centre> <a href=@{FromWebR Cancel}>Finish </a>
  $forall row <- rows
   <div .row id=#{show row}>
    $forall celd <- row  
      <div .celd name=#{fst celd}>
        <a href=@{FromWebR (Movement $ fst celd)}>
           $if snd celd == 1
              <img src=@{StaticR red_png} height="75px" width="75px">
           $elseif snd celd == minusone
              <img src=@{StaticR yellow_png} height="75px" width="75px">
           $else
              <img src=@{StaticR empty_png} #updategrid height="75px" width="75px">
$nothing
 <div .centre> <a href=@{AuthR LoginR}>Go to the login page
